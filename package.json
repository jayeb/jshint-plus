{
  "name": "reporter-plus",
  "version": "1.3.0",
  "description": "A reporter for JSHint and JSCS with stylized output and notifications",
  "files": [
    "jshint",
    "jscs",
    "lib"
  ],
  "scripts": {
    "jshint": "jshint --reporter=jshint/jshint.js ./lib",
    "jscs": "jscs --reporter=jscs/index.js ./lib"
  },
  "author": {
    "name": "Jason Eberle",
    "email": "eberle.jason@gmail.com"
  },
  "license": "MIT",
  "repository": "jayeb/reporter-plus",
  "dependencies": {
    "chalk": "^1.0.0",
    "easy-table": "^0.3.0",
    "lodash": "^3.8.0",
    "log-symbols": "^1.0.2",
    "node-notifier": "^4.2.1"
  },
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-contrib-jshint": "^0.11.2",
    "grunt-jscs": "^1.8.0",
    "jit-grunt": "^0.9.1",
    "jscs": "^1.13.1",
    "jshint": "^2.7.0",
    "precommit-hook": "^2.0.1",
    "time-grunt": "^1.2.0"
  },
  "pre-commit": [
    "jshint",
    "jscs"
  ]
}
